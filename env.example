# Deployment Configuration
# Copy this file to .env and fill in the values

# Private key for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# HyperEVM RPC URL
RPC_URL=https://rpc.hyperliquid.xyz/evm

# HYPE token address (if using ERC-20 burn mode)
HYPE_TOKEN=0x0000000000000000000000000000000000000000

# Initial owner of the NFT contract
INITIAL_OWNER=0x0000000000000000000000000000000000000000

# Whether to use native HYPE mode initially (true/false)
USE_NATIVE_MODE=false

# Etherscan API key for contract verification (if available)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Frontend Configuration
# WalletConnect Project ID for mobile wallet support
# Get your project ID from https://cloud.walletconnect.com
VITE_WALLETCONNECT_PROJECT_ID=your_project_id_here
